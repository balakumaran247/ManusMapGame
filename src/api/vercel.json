{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.10.14"
            }
        }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/main.py" }],
    "env": {
        "APP_MODULE": "main:app"
    }
}
